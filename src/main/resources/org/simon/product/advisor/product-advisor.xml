<productAdvisor>
	<id>advisor-1</id>
	<category>mobile-phones</category>
	<meta>
		<name>meta1</name>
		<value>value1</value>
	</meta>
	<filterGroup>
		<id>filterGroup1</id>
		<questionStyle>qs</questionStyle>
		<logicalOperator>And</logicalOperator>
		<meta>
			<name>meta1</name>
			<value>value1</value>
		</meta>
		<filter>
			<id>filterGroup1-filter1</id>
			<meta>
				<name>controlType</name>
				<value>range</value>
			</meta>
			<meta>
				<name>slide-step</name>
				<value>100</value>
			</meta>
			<meta>
				<name>min-label</name>
				<value>0 </value>
			</meta>
			<meta>
				<name>max-label</name>
				<value>5000 AUD</value>
			</meta>
			<meta>
				<name>min</name>
				<value>0</value>
			</meta>
			<meta>
				<name>max</name>
				<value>5000</value>
			</meta>
			<meta>
				<name>init-min</name>
				<value>300</value>
			</meta>
			<meta>
				<name>init-max</name>
				<value>1800</value>
			</meta>
			<rule>
				<type>range</type>
				<value><![CDATA[product.price> min && product.price<max]]></value>
			</rule>
		</filter>
	</filterGroup>
	<filterGroup>
		<id>filterGroup2</id>
		<questionStyle>qs</questionStyle>
		<logicalOperator>And</logicalOperator>
		<meta>
			<name>meta1</name>
			<value>value1</value>
		</meta>
		<filter>
			<id>filterGroup2-filter1</id>
			<meta>
				<name>controlType</name>
				<value>checkbox</value>
			</meta>
			<meta>
				<name>label</name>
				<value>Carrier : Cellular South </value>
			</meta>
			<rule>
				<type></type>
				<value>"cellular south".equalsIgnoreCase(product.carrier)</value>
			</rule>
		</filter>
		<filter>
			<id>filterGroup2-filter2</id>
			<meta>
				<name>controlType</name>
				<value>checkbox</value>
			</meta>
			<meta>
				<name>label</name>
				<value><![CDATA[Carrier : AT&T ]]></value>
			</meta>
			<rule>
				<type></type>
				<value><![CDATA["at&t".equalsIgnoreCase(product.carrier)]]></value>
			</rule>
		</filter>
	</filterGroup>
	<filterGroup>
		<id>filterGroup3-filter1</id>
		<questionStyle>qs</questionStyle>
		<logicalOperator>And</logicalOperator>
		<meta>
			<name>meta1</name>
			<value>value1</value>
		</meta>
		<filter>
			<id>filter2</id>
			<meta>
				<name>controlType</name>
				<value>checkbox</value>
			</meta>
			<meta>
				<name>label</name>
				<value>Dell Phone</value>
			</meta>
			<rule>
				<type></type>
				<value>product.name.toLowerCase().contains("dell")</value>
			</rule>
		</filter>
		<filter>
			<id>filterGroup3-filter2</id>
			<meta>
				<name>controlType</name>
				<value>checkbox</value>
			</meta>
			<meta>
				<name>label</name>
				<value>Samsung Phone</value>
			</meta>
			<rule>
				<type></type>
				<value>product.name.toLowerCase().contains("samsung")</value>
			</rule>
		</filter>
	</filterGroup>
</productAdvisor>